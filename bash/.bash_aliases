alias browser='firefox >/dev/null 2>&1 & disown'
alias chrome='google-chrome-stable >/dev/null 2>&1 & disown'
alias grep='grep --color=auto'
alias ls='ls --color=auto'
alias spotify='spotify >/dev/null 2>&1 & disown'

# PC guide
alias rgb='openrgb >/dev/null 2>&1 & disown'
alias rgb_off='openrgb -c 000000 >/dev/null 2>&1'
alias rgb_red='openrgb -c FF0000 >/dev/null 2>&1'
alias rgb_white='openrgb -c FFFFFF >/dev/null 2>&1'
